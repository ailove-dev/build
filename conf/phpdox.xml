<phpdox xmlns="http://phpdox.de/config">
    <project name="equip" source="src" workdir="../../build/phpdox">
    <collector publiconly="false">
      <include mask="*.php" />
      <exclude mask="*autoload.php" />
    </collector>

    <generator output="../../build">
      <build engine="html" enabled="true" output="api"/>
    </generator>
  </project>
</phpdox>
